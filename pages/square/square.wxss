/* pages/square/square.wxss */
.square-container {
  width: 100%;
  height: 100vh;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
}

.square-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 64rpx;
  text-align: center;
}

.square-title {
  font-size: 40rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 16rpx;
}

.square-desc {
  font-size: 28rpx;
  color: #666;
}
