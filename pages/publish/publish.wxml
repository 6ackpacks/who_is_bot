<!--pages/publish/publish.wxml-->
<view class="publish-container">
  <view class="publish-content">
    <!-- 上传图标 -->
    <view class="upload-icon">
      <text class="icon-plus">+</text>
    </view>

    <!-- 标题 -->
    <view class="publish-title">上传样本</view>

    <!-- 描述 -->
    <view class="publish-desc">贡献疑似 AI 生成的内容，让社区来判定。</view>

    <!-- 返回按钮 -->
    <button class="back-btn" bindtap="handleBack">返回</button>
  </view>
</view>
